<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml">
    <TextField fx:id="inputTextField" promptText="Введите выражение" />
    <Button fx:id="calculateButton" text="Вычислить" onAction="#handleCalculateButtonAction"/>
    <TextField fx:id="resultTextField" editable="false" />
</VBox>